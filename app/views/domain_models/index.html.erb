<p id="notice"><%= notice %></p>

<h1>Domain Models</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @domain_models.each do |domain_model| %>
      <tr>
        <td><%= domain_model.name %></td>
        <td><%= domain_model.desc %></td>
        <td><%= link_to 'Show', domain_model %></td>
        <td><%= link_to 'Edit', edit_domain_model_path(domain_model) %></td>
        <td><%= link_to 'Destroy', domain_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Domain Model', new_domain_model_path %>
